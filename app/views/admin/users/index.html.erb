<h1>整站用户列表</h1>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <td>ID</td>
      <td>手机号</td>
      <td>注册时间</td>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.phone %></td>
        <td><%= user.created_at.strftime('%Y-%m-%d %H:%M:%S') %></td>
      </tr>
    <% end  %>
  </tbody>
</table>
